{
  "name": "itcss",
  "version": "1.0.0",
  "description": "A template styles follow ITCSS style",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run sass:dev\" \"live-server public/ \"",
    "sass": "sass src/scss:public/styles",
    "sass:dev": "sass --watch --embed-source-map src/scss:public/styles",
    "sass:prod": "sass --no-source-map --style compressed src/scss:public/styles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DungGramer/ITCSS.git"
  },
  "keywords": [
    "ITCSS",
    "SCSS",
    "SASS",
    "CSS",
    "CSS3"
  ],
  "author": "DungGramer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DungGramer/ITCSS/issues"
  },
  "homepage": "https://github.com/DungGramer/ITCSS#readme",
  "devDependencies": {
    "concurrently": "7.2.2",
    "live-server": "1.2.2",
    "sass": "^1.52.3"
  }
}
